<verb Label="return to reserve" Witnesses="NOBODY_AT_ALL">
<description>The intervener (usually Arthur) terminates the intervention and returns to his reserve position.</description>
<hijackable>false</hijackable>
<occupiesDirObject>false</occupiesDirObject>
<timeToPrepare>1</timeToPrepare>
<timeToExecute>1</timeToExecute>
<category>battle scene</category>
